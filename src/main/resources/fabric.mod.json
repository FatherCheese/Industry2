{
  "schemaVersion": 1,
  "id": "industry",
  "version": "${version}",

  "name": "Industry2",
  "description": "This mod adds industrial machines for your minecraft world.",
  "authors": [
    "Turnip Labs",
    "Cookie Cakes"
  ],
  "contact": {
    "homepage": "https://github.com/FatherCheese/Industry2",
    "sources": "https://github.com/FatherCheese/Industry2",
    "issues": "https://github.com/FatherCheese/Industry2/issues"
  },

  "license": "CC0-1.0",

  "icon": "assets/industry/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "turniplabs.industry.Industry2"
      }
    ]
  },
  "mixins": [
    "industry.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.7",
    "fabric-language-kotlin": "*",
    "halplibe": ">=2.1.5",
    "sunsetutils": ">=1.7.1",
    "energyapi": ">=1.1.4"
  },
  "suggests": {
  }
}